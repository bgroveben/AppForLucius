<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.appforlucius" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>AppForLucius</name>
    <description>
        This is a photo album about Lucius.
    </description>
    <author email="bgroveben@gmail.com" href="https://bgroveben.github.io/">
        Benjamin Grove
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
